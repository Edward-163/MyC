void test01()
{
    int i = -1;
    // int i=3;
    /// 考点:sizeof返回类型unsigned int   比较符号时自动类型转换
    for (; i < sizeof(int); i++) {
        printf("%d \n", i);fflush(stdout);
    }
    printf("%s \n","over");fflush(stdout);
}
int main()
{
    // @formatter:off
    test01();
    return 0;
    // @formatter:on
}
